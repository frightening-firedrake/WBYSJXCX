.card_list_section .card_list_con .panel_list{
  width: 46%;
  background: #fff;
  border-radius: 10rpx;
  padding:0 20rpx;
  box-sizing: border-box;
  position: relative;
  box-shadow: 0 0 8px rgba(0,0,0,0.1);
 
}
.card_list_section .card_list_con .panel_list .imgbox{
  position: relative;
  text-align: center;
  height: 320rpx;
  padding-top:120rpx;
}
.card_list_section .card_list_con .panel_list .imgbox .thumbnail{
  width: 240rpx;
  height: 320rpx;
}
.card_list_section .card_list_con .panel_list .imgbox .price-reduction{
  background-image:url(data:image/png;base64,);
  width:100%;
  height: 58rpx;
  line-height: 58rpx;
  background-repeat:no-repeat ;
  background-position: center;
  background-size: 100%;
  position:absolute;
  left:50%;
  transform: translateX(-50%);
  bottom:0;
  color:#fff;
  text-align: left;
  box-sizing: border-box;
  font-size: 26rpx;
  padding-left:1em;
}
.panel_list .imgbox .price-reduction text{
  font-size: 34rpx;
}
.card_list_section .card_list_con .panel_list .info{
  padding:10rpx;
  position: relative;
}
.card_list_section .card_list_con .panel_list .info .title{
  font-size:28rpx;
  color: #000;
  font-weight: 500;
}
.card_list_section .card_list_con .panel_list .info .tag{
  font-size:22rpx;
  color: #333;
  display: flex;
  gap: 10rpx;
  line-height: 40rpx;
}
.card_list_section .card_list_con .panel_list .info .price{
  font-size:30rpx;
  color: #FE3B34;
  display: flex;
  font-weight: 500;
  line-height: 50rpx;
  display: flex;
}
.card_list_section .card_list_con .panel_list .info .price text{
  font-size:22rpx;
  color: #999;
  text-decoration: line-through;
  padding-left: 0.5em;
}
.card_list_section .card_list_con .panel_list .info .opening_count{
  font-size:22rpx;
  color: #999;
  flex: 1;
  text-align: right;
}
.card_list_section .card_list_con .panel_list .card_state{
  font-size:22rpx;
  color: #999;
  position: absolute;
  left: 36rpx;
  top: 46rpx;
  color: #fff;
  padding:5rpx 10rpx;
  background-color: #F4B975;
  border-radius: 4px;
  font-weight: 700;
}
.card_list_section .card_list_con .panel_list .card_state2{
  background-color: #FE3B34;
}
