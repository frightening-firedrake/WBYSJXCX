/* pages/carpoolingRecord/index.wxss */
.rili{
  background-image:url(data:image/svg+xml;base64,);
  width:32rpx;
  height:32rpx;
}
.page_app{
  padding-bottom:0;
}
.order_page_section{
  padding: 0 28rpx;
}
.page_app .cards_search_con .share_btn{
  font-size: 24rpx;
}
.order_tab_navbar{
  display: flex;
  justify-content:space-around;
  font-size:28rpx;
  margin-bottom: 20rpx;
}
.order_tab_item{
  height: 60rpx;
  line-height: 60rpx;
  border-bottom: solid 2px transparent;
  transition: all linear 0.2s;
}
.order_tab_item.active{
  border-bottom-color: var(--theme-color);
  color: var(--theme-color);
}


/* 订单卡片样式 */
.order_list_section{
  padding:0;
}
.order_li_con{
  background-color: #ffffff;
  box-shadow: 0px 0px 10px #e7e7e7;
  border-radius: 14rpx;
  margin-bottom: 20rpx;
}
.order_li_top{
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: solid 1px #f1f1f1;
  padding: 0 34rpx;
  line-height:80rpx;
  height:80rpx;
}
.order_li_num{
  font-size: 28rpx;
  font-weight: bold;
  color:#333;
  width: 18em;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.order_li_state{
  font-size: 28rpx;
  color: #333;
  font-weight: bold;
}
.order_li_infor{
  padding: 25rpx 30rpx;
}
.order_infor_ba{
  display: flex;
  flex-direction: column;
}
.order_infor_store,.order_infor_time{
  font-size: 24rpx;
  color: #333333;
}
.store_icon{
  font-size: 14px;
  color: var(--theme-color);
  margin-right: 10rpx;
}
.order_infor_con{
  /* display: flex;
  justify-content: space-between; */
  position: relative;
}
.order_infor_list{
  /* margin-right: 20rpx;
  flex: 1;
  display: flex;
  align-items: center;
  flex-wrap: wrap; */
  display: flex;
  flex-direction: column;
}
.order_infor_list .order_infor_li{
  display: flex;
  font-size: 28rpx;
  color: #333333;
  font-weight: bold;
  height:46rpx;
  line-height:46rpx;
  /* width: 50%; */
}
.order_infor_li{
  padding-right: 24rpx;
  box-sizing: border-box;
  line-height: 60rpx;
}
.order_infor_li:nth-child(2n){
  margin-right: 0;
}
.order_infor_label{
  color: #666;
  font-weight: normal;
  width: 5em;
  text-align: justify;
  text-align-last: justify;
}
.order_price{
  position: absolute;
  font-size: 28rpx;
  color: #F53A3A;
  right: 0;
  bottom: 0;
  line-height: 46rpx;
}
.card_list_scroll{
  /* height:calc(100vh - 196rpx); */
  /* height: calc(100vh - 72rpx - 1px - 28rpx - 20rpx - 2px - 60rpx - 56px); */
  /* height: calc(100vh - 100rpx - 80rpx - 140rpx - 16rpx); */
  /* height: calc(100vh - 400rpx - 20rpx - 16rpx); */
  height: calc(100vh - 144rpx - 274rpx - 20rpx - 16rpx);
  margin-top: 20rpx;
  /* margin:0 -7px; */
  width: auto;
  /* border-top:1px solid rgba(103,103,103,0.1) ; */
}
.tabL{
  height:64rpx;
  padding:30rpx 64rpx;
  text-align: center;
}
.tabL .tab{
  display: inline-block;
  height:64rpx;
  width:56%;
  border-radius: 32rpx;
  text-align: center;
  line-height:64rpx;
  background-color: #E8E5E9;
  margin:0 -4%;
  color:#333;
  font-size:26rpx;
}
.tabL .top{
  position: relative;
  z-index: 1;
}
.tabL .active{
  background-color: var(--theme-color);
  color:#fff;
}
.inputWrap{
  height:64rpx;
  border:1px solid rgb(232, 234, 236);
  position: relative;
  margin-bottom:24rpx;
}
.filter_list_header_left{
  color:#333;
  font-size:32rpx;
  line-height:116rpx;
}
.lable{
  color:#333;
  font-size:28rpx;
  line-height:84rpx;
}
.inputWrap input{
  width:100%;
  height:100%;
  box-sizing: border-box;
  padding:0 50rpx 0 20rpx;
  font-size:24rpx;
}
.inputWrap .rili{
  position: absolute;
  right:20rpx;
  top:16rpx;
}
.page_app .filter_box .filter_con .from_view_btn .filter_btn {
  position: absolute;
  left:32rpx;
  right:32rpx;
  bottom:10rpx;
  width: auto;
}