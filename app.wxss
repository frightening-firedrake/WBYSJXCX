/* 左滑筛选 */
.filter_box{
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
}
.filter_box.active .filter_con{
  transform: translateX(0);
}
.filter_con{
  position: fixed;
  left: 100rpx;
  right: 0;
  top: 0;
  bottom: 0;
  background-color: #fff;
  box-sizing: border-box;
  padding: 20rpx 32rpx;
  z-index:999999;
  transform: translateX(100%);
  transition: all 0.2s linear;
  border-radius: 8px 0 0 8px;
}
.filter_back{
  margin-bottom: 20rpx;
}
.filter_back_icon{
  transform: rotate(180deg);
  font-size: 16px;
  color: #666666;
}

.dialog_dialog__mask{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 10;
  background: rgba(0, 0, 0, 0.7);
  display: none;
 }
 .filter_box.active .dialog_dialog__mask{
   display: block;
   z-index:99999;
 }
