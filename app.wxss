  padding: 0rpx 34rpx;
  height:116rpx;
}
page .newList .weui-slideview__right{
  /* margin-left:34rpx; */
}
page .newList .iconbefore{
  font-size: 46rpx;
  margin-right: 30rpx;
  line-height: 50px;
  position: relative;
}
page .newList .iconbefore::after{
  content: "";
  position: absolute;
  right: 0px;
  top: 25px;
  width: 12px;
  height: 12px;
  border-radius: 50%;
  background-color: rgba(162,168,198,0.4);
}




