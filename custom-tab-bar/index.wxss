/* custom-tab-bar/index.wxss */
.tab-bar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 9999;
  height: 140rpx;
  display: flex;
  /* background-color: white; */
  /* background-color: red; */
  background-image: url(data:image/png;base64,);
  padding-bottom: env(safe-area-inset-bottom);
  box-shadow: 0 0 -10px rgba(0, 0, 0, 0.3);
  /* padding-top: 10px; */
  bottom: 0;
  /* padding:4px 0; */
  background-size: auto 140rpx;
  background-position: bottom center;
}

.tab-bar-item {
  flex: 1;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  font-size: 20rpx;
}

.tab-bar-item:nth-child(4) .bar-item-img{
  width: 112rpx;
  height: 112rpx;
  line-height: 112rpx;
  /* border: solid 1px #dadada; */
  /* border: solid 1px rgba(88,118,255, 0.5); */
  /* box-shadow:0 0 5px rgba(0, 0, 0, 0.3); */
  /* box-shadow:0 0 10px rgba(88,118,255, 0.3); */
  border-radius: 50%;
  /* background-color: #fff; */
  background-color: #7089FF;
  /* background-color: #619CF7; */
  position: relative;
  bottom: 86rpx;
  margin: 0 auto;
  display: flex;
  justify-content: center;
  align-items: center;
  color:#fff;
}
.tab-bar-item:nth-child(4) .cover-image{
  width: 20px;
  height: 20px;
  margin: auto;
}
.tab-bar-item .cover-image {
  width: 24px;
  height: 24px;
  margin:0 auto 3px;
}


.data_num{
  font-weight: bold;
}